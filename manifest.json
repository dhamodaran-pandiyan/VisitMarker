{
    "manifest_version": 3,
    "name": "VisitMarker",
    "version": "0.1",
    "description": "Show last visit information for domains on Google search results.",
    "permissions": ["history", "scripting"],
    "host_permissions": ["https://*.google.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.google.com/search*"],
        "js": ["content.js"]
      }
    ]
  }
  